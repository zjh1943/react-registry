{
  "name": "react-registry",
  "version": "0.1.2",
  "description": "A library for registering, retrieving, and rendering React components.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "lint": "node node_modules/tslint/bin/tslint -c tslint.json -t codeFrame 'src/**/*.{ts,tsx}'",
    "lint:fix": "node node_modules/tslint/bin/tslint -c tslint.json -t codeFrame 'src/**/*.{ts,tsx}' --fix",
    "test": "jest",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:cover && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "clean-dist": "npm run remove-dist",
    "remove-dist": "rimraf ./dist/*",
    "prebuild": "npm run lint && npm run remove-dist",
    "build": "node ./node_modules/typescript/bin/tsc --outDir ./dist"
  },
  "author": "Joe Esposito <joe@devnet.io>",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/jest": "20.0.8",
    "@types/lodash": "4.14.74",
    "@types/node": "^9.6.7",
    "@types/prop-types": "15.5.1",
    "@types/react": "16.0.5",
    "coveralls": "3.0.0",
    "jest": "21.1.0",
    "jest-cli": "21.1.0",
    "jsdoc": "3.5.5",
    "rimraf": "2.6.1",
    "ts-jest": "21.0.1",
    "tslint": "5.7.0",
    "typedoc": "^0.9.0",
    "typedoc-plugin-markdown": "1.0.11",
    "typescript": "2.8.3",
    "prop-types": "15.5.10",
    "react": "15.0.2"
  },
  "peerDependencies": {
    "react": "^14.0.0 || ^15.0.0",
    "prop-types": "^15.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "spec.*\\.(ts|tsx|js|jsx)$"
  },
  "bugs": {
    "url": "https://github.com/devnet-io/typescript-graphql/issues"
  },
  "homepage": "https://devnet.io/libs/react-registry",
  "repository": {
    "type": "git",
    "url": "https://github.com/devnet-io/react-registry.git"
  }
}
